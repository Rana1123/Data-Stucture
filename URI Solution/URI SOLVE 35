#include<stdio.h>

int main()
{
    int A,B,C,D,sum1=0,sum2=0;

    scanf("%d",&A);

    scanf("%d",&B);

    scanf("%d",&C);

    scanf("%d",&D);

    if(B>C  &&  D>A)
    {
        if(A%2==0)
        {
            sum1=A+B;
            sum2=C+D;
            if(sum1<sum2)
            {
                if(sum2>0)
                {
                    printf("Valores aceitos\n");
                }
                else
    {
        printf("Valores nao aceitos\n");
    }
            }
            else
    {
        printf("Valores nao aceitos\n");
    }
        }
        else
    {
        printf("Valores nao aceitos\n");
    }
    }
    else
    {
        printf("Valores nao aceitos\n");
    }
}

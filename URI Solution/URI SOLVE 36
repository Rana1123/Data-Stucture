#include<stdio.h>

int main()

{
    double a,b,c,R1,R2,D;

    scanf("%lf %lf %lf",&a,&b,&c);

    D=(b*b)-(4*a*c);

    if(a==0  || D<0)

    {
        printf("Impossivel calcular\n");
    }

    else
    {
        R1=(-b+sqrt(D))/(2*a);

        R2=(-b-sqrt(D))/(2*a);

        printf("R1 = %.5lf\n",R1);
        printf("R2 = %.5lf\n",R2);
    }
}

#include<stdio.h>

int main()
{
    int a,b,sum,sum1;

    scanf("%d %d",&a,&b);

    if(a>=0 && a<=24 && b>=0 && b<=24)
    {
        if(a>=b)
        {
            sum=24-a;
            sum1=sum+b;
            printf("O JOGO DUROU %d HORA(S)\n",sum1);
        }
        else
        {
            sum=b-a;
            printf("O JOGO DUROU %d HORA(S)\n",sum);
        }
    }
}
